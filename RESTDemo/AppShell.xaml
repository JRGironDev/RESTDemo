<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RESTDemo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RESTDemo.Views"
    xmlns:restDemo="clr-namespace:RESTDemo"
    Shell.FlyoutBehavior="Disabled"
    Title="RESTDemo">

    <ShellContent
        Title="OnBoarding"
        ContentTemplate="{DataTemplate local:OnBoarding}"
        Route="OnBoarding" />
    
    <ShellContent
        Title="ApiApiPage"
        ContentTemplate="{DataTemplate restDemo:ApiPage}"
        Route="ApiApiPage" />

</Shell>
